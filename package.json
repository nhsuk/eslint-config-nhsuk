{
  "name": "eslint-config-nhsuk",
  "version": "0.14.0",
  "description": "ESLint config. Primarily intended for use within NHSUK projects.",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "yarn eslint .",
    "precommit": "yarn lint && yarn test",
    "prepush": "yarn precommit && yarn snyk test",

    "eslint",
    "eslintconfig",
    "NHSUK"
  ],
  "author": "steven hunt",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=4.0.0",
    "eslint-config-airbnb-base": ">=12.0.0",
    "eslint-plugin-json": ">=1.0.0",
    "eslint-plugin-mocha": ">=4.0.0"
  },
  "dependencies": {
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-json": "^1.0.0",
    "eslint-plugin-mocha": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "husky": "^0.14.0",
    "mocha": "^5.0.0",
    "snyk": "^1.0.0"
  }
}
